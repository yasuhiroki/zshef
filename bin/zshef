#!/usr/bin/env zsh

function zshef(){
    local arg="$1"

    case "$arg" in
        init | run | install | update)
            shift
            zshef::core::interface::command \
                "$arg" "$@"
            ;;

        *)
            zshef::help
            ;;
    esac
}

source ${${(%):-%N}:A:h}/../init.sh
zshef ${@}
